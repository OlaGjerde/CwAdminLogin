{
  "name": "awslogin",
  "private": true,
  "version": "0.10.40",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/gen-build-info.cjs",
    "build": "npm run bump:patch && tsc -b && node scripts/gen-build-info.cjs && vite build",
    "bump:patch": "node scripts/version-bump.cjs patch",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "aws s3 rm s3://calwin-login/dev --recursive & aws s3 sync dist/ s3://calwin-login/dev & aws cloudfront create-invalidation --distribution-id=E2NUBJ1FIGIIPV --paths /"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "devextreme": "^25.1.3",
    "devextreme-react": "^25.1.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/pako": "^2.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
